Discount INT NOT NULL DEFAULT '0',
FOREIGN KEY (OrderID) REFERENCES Orders(OrderID),
FOREIGN KEY (BookName) REFERENCES Books(BookName)
);</textarea></div>
            <div class="output" data-markdown="
"></div>
          </div>
